import { EventEmitter, OnChanges, OnInit } from '@angular/core';
import { GameManagerService, Tile } from './services/game-manager.service';
export declare enum GameState {
    Paused = 0,
    Started = 1,
    Over = 2,
}
export declare class TetrisCoreComponent implements OnInit, OnChanges {
    private _manager;
    tileSize: any;
    initialSpeed: number;
    rotate: boolean;
    moveLeft: boolean;
    moveRight: boolean;
    moveDown: boolean;
    start: boolean;
    stop: boolean;
    reset: boolean;
    lineCleared: EventEmitter<any>;
    gameOver: EventEmitter<any>;
    grid: Array<Tile>;
    state: GameState;
    gridWidth: number;
    gridHeight: number;
    private _moveDownSpeed;
    constructor(_manager: GameManagerService);
    ngOnInit(): void;
    ngOnChanges(changes: any): void;
    actionLeft(): void;
    actionRight(): void;
    actionRotate(): void;
    actionDown(): void;
    actionReset(): void;
    actionStart(): void;
    actionStop(): void;
    private _keyPressed(key);
    private _onLineCleared(data);
    private _onGameOver(data);
}
